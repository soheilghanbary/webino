    "std": "prisma studio"
  },
  "dependencies": {
    "@hono/node-server": "^1.11.5",
    "@prisma/client": "5.16.1",
    "bcryptjs": "^2.4.3",
    "concurrently": "^8.2.2",
    "dotenv": "^16.4.5",
    "hono": "^4.4.10"
  },
  "devDependencies": {
    "@types/node": "^20.14.9",
    "esbuild": "^0.23.0",
    "prisma": "^5.16.1",
    "tsx": "^4.16.0",
    "typescript": "^5.5.3"
  },
  "prisma": {
    "schema": "server/db/schema.prisma"
  }
}
