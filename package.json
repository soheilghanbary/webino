{
  "name": "mockstack",
  "version": "1.0.0",
  "main": "server/index.ts",
  "scripts": {
    "start:dev": "tsx watch server/index.ts",
    "dev": "concurrently \"cd client && pnpx vite\" \"tsx watch server/index.ts\"",
    "build": "pnpm run gen && tsx ./build.ts",
    "start": "node dist/server.cjs",
